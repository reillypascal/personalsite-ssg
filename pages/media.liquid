<h1 class="sectionHeader">Media</h1>

<article>
    <div class="videoGrid">
        {%- for video in media.videos %}
            <div class="containingBlock">
                <h2 class="video-link">
                    <a href="https://www.youtube.com/watch?v={{ video.yt_id }}"
                        ><cite>{{ video.title }}</cite></a
                    >
                </h2>
                {%- if video.peertube_link %}
                    <p><a href="{{ video.peertube_link }}">PeerTube link</a></p>
                {%- endif %}
                <div class="videoWrapper">
                    <lite-youtube
                        videoid="{{ video.yt_id }}"
                        style="background-image: url('https://i.ytimg.com/vi/{{ video.yt_id }}/hqdefault.jpg');"
                    >
                    </lite-youtube>
                </div>
            </div>
        {% endfor %}
        <!-- close "videoGrid" -->
    </div>
</article>


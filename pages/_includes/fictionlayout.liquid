---
layout: mainlayout.liquid
title: Reilly Spitzfaden, Composer
---
<link rel="stylesheet" type="text/css" href="/styles/blog.css">
<link rel="stylesheet" type="text/css" href="/styles/blogposts.css">
<link rel="stylesheet" type="text/css" href="/styles/fiction.css">
<link rel="stylesheet" type="text/css" href="/styles/onecolumn.css">

<!-- post contents -->
<article class="h-entry">
    <h1 class="p-name sectionHeader">{{ title }}</h1>

    {% include 'partials/author-and-syndication.liquid' %}

    <div class="post-meta">
        <span class="meta-icon">
            <img src="/media/icon-calendar-ltr.svg" alt="calendar icon" width="18" height="18">
        </span>

        <span class="meta-text">
            <a class="u-url" href="{{ url }}"
                ><time class="dt-published" datetime="{{ date | postDate }}">
                    {{- date | date: '%A, %B %d, %Y' -}}
                </time></a
            >
        </span>
    </div>

    <div class="e-content">
        {{ content }}
        <!-- <div class="centered asterism">‚ÅÇ</div> -->
        {%- comment %}
            {% include "partials/post-interactions.liquid" %}
        {%- endcomment %}
        {%- comment %}
            <div class="email-reply">
                <span class="mail-icon">
                    <img width="18" height="18" src="/media/ui/mail-icon-light-18px.svg" alt="mail icon" />
                </span>
                <a href='mailto:reillypascal@gmail.com?subject=Re: {{ title }}'>Reply via email</a>
            </div>
        {%- endcomment %}
    </div>
</article>
